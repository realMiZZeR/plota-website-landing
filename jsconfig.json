{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "paths": {
      "@/*":  ["./src/*"],
      "@/styles/*": ["src/assets/styles/*"],
      "@/fonts/*": ["src/assets/fonts/*"],
      "@/images/*": ["src/assets/images/*"],
    },
  },
  "include": ["src/**/*.scss", "src/**/*.jsx"],
  "exclude": ["node_modules"],
}
